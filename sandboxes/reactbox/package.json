{
  "name": "reactbox",
  "version": "0.1.0",
  "private": true,
  "watch": {
    "copy": "../../src/**/*"
  },
  "dependencies": {
    "@lit-labs/react": "^1.1.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.16",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.9.0",
    "react-scripts": "5.0.1",
    "sass": "^1.59.3",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "rs:build": "react-scripts build",
    "rs:eject": "react-scripts eject",
    "rs:start": "react-scripts start",
    "rs:test": "react-scripts test",
    "start": "concurrently npm:rs:start npm:tsc:watch",
    "tsc": "tsc",
    "tsc:watch": "tsc -w",
    "watch": "npm-watch"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "ignorePatterns": [
      "src/build/**/*"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.21",
    "npm-watch": "^0.11.0"
  }
}
